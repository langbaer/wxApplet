/* subpkg/search/search.wxss */
image {
  width: 100%;
  height: 100%;
}
.searchFrame {
  position: sticky;
  top:0;
  z-index: 999;
}
.historyFrame {
  width: 100%;
  background-color: #07c160;
}
.history {
  width: 100%;
  background-color: #ffffff;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.nav {
  width: 100%; 
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
}
.text {
  margin: 9rpx 0;
  color: rgb(167, 167, 167);
}
.img {
  width: 48rpx;
}
.boxFrame {
  position: absolute;
  display: flex;
  flex-flow: row wrap;
  justify-content: left;
  align-content: space-around;
  width: 100%;
}
.box {
  padding: 10rpx 30rpx;
  background-color: rgb(167, 167, 167);
  border-radius: 30rpx;
  font-size: 24rpx;
  color: #ffffff;
  margin: 10rpx;
}