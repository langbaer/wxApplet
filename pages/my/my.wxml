    <view class="box22">
      <view class="box22Pic"><image src="/image/my/allOrders.png" mode="widthFix"></image></view>
      <view class="box2Text">全部订单</view>
    </view>
  </view>
  </view>
  <view class="box3">
    <view class="box33" bindtap="navToAddress">收货地址</view>
    <view class="box33">联系客服</view>
    <view class="box33" bindtap="outLogin1">退出登入</view>
  </view>
