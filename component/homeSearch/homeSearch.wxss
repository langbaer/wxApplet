/* component/homeSearch/homeSearch.wxss */
.frame  {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100rpx;
  /* background-color: #3cc51f; */
}
.searchFrame {
  position: relative;
  display: flex;
  align-items: center;
  width: 95%;
  height: 72rpx;
  background-color: #ffffff;
  /* border-radius: 30px; */
  padding: 0 30rpx;
  box-sizing: border-box;
}
.shouldDisplay {
  display: flex;
  position:absolute;
  left: 50%;
  transform: translateX(-50%);
}
.shouldDisplayIcon {
  width: 30rpx;
  height: 50rpx;
}
.shouldDisplayText {
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}
.searchIput {
  width: 100%;
}