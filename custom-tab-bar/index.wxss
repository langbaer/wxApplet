/* custom-tab-bar/index.wxss */
.frame {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 150rpx;
  border-top:5rpx solid #7A7E83;
  background-color: white;
}

.box {
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;

}
.pic {
  width: 80rpx;
  height: 80rpx;

}
image {
  width: 100%;
}
.text {
  width: 80rpx;
  text-align: center;
  font-size: 25rpx;
}